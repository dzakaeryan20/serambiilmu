<meta name="viewport" content="width=device-width, initial-scale=1.0">
<style>
    .jumbotron {
        padding-top: 6rem;
        padding-bottom: 6rem;
        margin-bottom: 0;
        background-color: #fff;
      }
      
      .jumbotron p:last-child {
        margin-bottom: 0;
      }
      
      .jumbotron-heading {
        font-weight: 300;
      }
      
      .jumbotron .container {
        max-width: 40rem;
      }
</style>
<nav class="navbar navbar-expand-lg navbar-light bg-light">
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
  
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav mr-auto">
        <li class="nav-item active">
          <a class="nav-link" href="#">Home <span class="sr-only">(current)</span></a>
        </li>
        <li class="nav-item">
            <a class="nav-link " href="/admin/header" tabindex="-1" aria-disabled="true">Header</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/admin/logout">Logout</a>
        </li>
        
      </ul>
      <form class="form-inline my-2 my-lg-0">
        <input class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search">
        <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
      </form>
    </div>
  </nav>
<section class="jumbotron text-center">
    <div class="container">
      <h1 class="jumbotron-heading">Serambi Ilmu</h1>
      <p class="lead text-muted"> List Content Artikel Untuk Serambi Ilmu</p>
      <p>
        <a href="/admin/create" class="btn btn-primary">Create</a>
        <a href="/" class="btn btn-secondary">View</a>
      </p>
    </div>
</section>
  
<div class="table-responsive" style="margin-top: 30px">
<table class="table">
  <thead>
    <tr>
      <th scope="col">No</th>
      <th scope="col">Judul</th>
      <th scope="col">Date</th>
      <th scope="col">File</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
    <% for(var i=0;i<data.length;i++){ %>
    <tr>
      <th scope="row"> <%= i+1 %></th>
      <td> <a href="admin/update/<%= data[i].id%>"><%= data[i].judul%></a></td>
      <td> <%= data[i].deskripsi%></td>
      <td> <a href="admin/download/<%= data[i].file%>"><%= data[i].file%></a></td>
      <td><a href="admin/delete/<%= data[i].id%>">Delete</a></td>
    </tr>
    <% } %>
  </tbody>
</table>
</div>